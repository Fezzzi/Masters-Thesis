    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack-node-externals": "^2.5.1"
  },
  "scripts": {
    "start": "set NODE_ENV=production && ts-node -r tsconfig-paths/register code/server/src",
    "dev": "concurrently --kill-others-on-fail -n web,api \"npm:start-web\" \"npm:start-api\"",
    "start-web": "webpack-dev-server --config config/webpack.config.js --open --mode development",
    "start-api": "nodemon -L --watch \"code/server/src/**\" --ext \"ts\" --exec \"ts-node -r tsconfig-paths/register code/server/src\"",
    "build": "rimraf dist/* && webpack --config config/webpack.config.js --mode production",
    "test": "npm run lint && npm run mocha",
    "lint": "eslint -c config/eslintrc.js . --ext .js,.tsx,.ts",
    "lint-fix": "eslint --fix -c config/eslintrc.js . --ext .js,.tsx,.ts",
    "mocha": "mocha -r ts-node/register -r tsconfig-paths/register -r ./config/mocha-env.config.js \"code/tests/**/*.spec.ts\"",
    "configure": "cp config/git-hooks/pre-push .git/hooks/pre-push"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
